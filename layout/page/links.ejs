<div class="flink" id="banners">
    <%- partial('partial/component/links/banner') %>
</div>
<div class="flink" id="article-container">
    <% site.data.links.links.forEach(data => { %>
        <h2><%= data.class_name %></h2>
        <div class="flink-desc"><%= data.descr %></div>
        <% switch (data.type) {
            case 'card': %>
                <%- partial('partial/component/links/linksCard',{data:data.link_list}) %>
            <% break;
            case 'item': %>
                <%- partial('partial/component/links/linksItem', {data:data.link_list}) %>
            <% break;
            default: %>
            <% break; } %> 
    <% }); %>
</div>
<div id="article-container">
    <%- page.content %>
</div>